{
    "name": "headsnet/doctrine-db-copy-bundle",
    "description": "Copy Doctrine databases without requiring mysqldump",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Ben Roberts",
            "email": "ben@headsnet.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/console": "^6.1",
        "doctrine/dbal": "^3.6",
        "symfony/http-kernel": "^6.1",
        "symfony/dependency-injection": "^6.1",
        "symfony/config": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Headsnet\\DoctrineDbCopyBundle\\": "src/"
        }
    }
}
